$("#guestRoomDrpdwn").html(calculateTotalGuest()+' guest'+(calculateTotalGuest()>1?'s':'')+' - '+totalRoomCount+' room'+(totalRoomCount>1?'s':''));}
function guestDecrement(Id){var textVal=$("#"+Id).val();var len=Id.length;var lastChar=Id.charAt(len-1);var rest=Id.slice(0,len-1);var persons=1;if(rest=="dChildrenCountSelector_Htls"){var persons=0;}
