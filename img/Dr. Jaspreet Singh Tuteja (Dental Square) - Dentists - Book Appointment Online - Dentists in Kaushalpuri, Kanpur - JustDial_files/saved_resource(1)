function ratingPaginationHtml(d,t)
{var html='';var lastpage='';var page='';var lpm1='';var fn='';var totrev='';if(t=='myrating')
{totrev=d.totrates;lastpage=d.mlastpage;page=d.pagert;lpm1=d.mlpm1;fn='mratings';}else if(t=='friendrating')
{totrev=d.totfriendreviews.total;lastpage=d.flastpage;page=d.pagert;lpm1=d.flpm1;fn='fratings';idn='fnrat';}else
{totrev=d.totrates;lastpage=d.lastpage;page=d.page;lpm1=d.lpm1;fn='ratings';idn='rat';}
if(typeof(d.company_name)=='undefined'){d.company_name='';}
