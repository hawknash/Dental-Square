function validateName(id,msg)
{var x=ed(id).value;if(!isNaN(x))
{ed(id+"Err").innerHTML=msg;$("#"+id+"Err").show();ed(id).focus();return false;}
if(!isValid(x,'name'))
{ed(id+"Err").innerHTML=msg;$("#"+id+"Err").show();ed(id).focus();return false;}
