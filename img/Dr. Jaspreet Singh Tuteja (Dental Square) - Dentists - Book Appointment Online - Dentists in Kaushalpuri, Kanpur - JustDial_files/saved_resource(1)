function postForgotPassword()
{var html_text='';var flid=$('#flid').val();var ajx_fURL=DOMAIN+"functions/forgotpasword_log.php";$.getJSON(ajx_fURL,{fgmob:flid,chkdnd:1},function(result){var data=result.results[0].msg;var redirect_to_register='';if(data=="MAX_LIMIT")
{html_text='You have reached your maximum limit of attempts for the day.Please try again later.';}
else if(data=="WRONG_LOGIN")
{html_text='No such account is registered with JustDial. Would you like to SignUp now?';redirect_to_register='1';}
else if(data=="SERVER_ERROR")
{html_text='Our servers have encountered an unexpected error, please try again after sometime!!';}
else if(data=="SUCCESS_MAIL")
{html_text='We have sent the password to you on your email '+flid+'.';}
else if(data=="SUCCESS_SMS")
{html_text='We have sent the password to you on your mobile number '+flid+'.';}
else if(data=="DAILY_LIMIT")
{html_text='Forgot password daily limit reached.';}
hideshowPopUp('jfp','','');$("#smsg").html(html_text);$("#redirectToRegister").val(redirect_to_register);hideshowPopUp('','password_reset','');});return false;}
function National_Search(){if($('#srchbxns').val()!=''&&$('#srchbxns').val()!="Search for anything, anywhere in India")
{var str=$.trim($('#srchbxns').val());str=str.replace(/[[\]{}()*+?&,\\^$|#]+/g,'');str=str.replace(/\.-/g,' ');str=str.replace(/[\s+]+/g,' ');str=str.toLowerCase();str=str.replace(/ /g,"-");window.location=DOMAIN+"National-Search/"+str;return false;}}
$('#srchbxns').keyup(function(event){if(event.which==13){National_Search();}});function homepageview(){SelectCity=(getCookie('scity'))?getCookie('scity'):'Mumbai';var now=new Date();var cur_time=now.getTime();if(PAGE_NAME=='index'){var autoValue=autoValuenew;}
$.ajax({url:WEBROOT+"webmain/ajxmain.php?cases=homepageview&city="+SelectCity+"&recodeData=5",city:SelectCity,onloadfn:onloadFn,date_t:cur_time,recodeData:$("a[id$='Movie']").length,async:true,success:function(result){SetMovieHotkey(result,'onload');}});}
function ngofun()
{var date=new Date();document.cookie='inweb_what='+document.index.what.value+'; '+date+'; path=/; domain='+cookieondomain;document.cookie='inweb_city='+$("#city").val().trim()+'; '+date+'; path=/; domain='+cookieondomain;$.ajax({url:WEBROOT+"webmain/ajxmain.php?city="+$("#city").val()+"&cases=citycheck",async:false,success:function(result){if(result=="City Failed")
{$("#city").focus();alert("Please Select Valid City");return false;}
else
{$('<form action="'+$('#nst').val().replace('URLDYNCITY',$('#city').val().replace(/[.,\/]+/g,''))+'"></form>').appendTo('body').submit();}}});}
function getCookie(c_name){if(document.cookie.length>0){var c_start=document.cookie.indexOf(" "+c_name+"=");if(c_start!=-1){c_start=c_start+c_name.length+2;var c_end=document.cookie.indexOf(";",c_start);if(c_end==-1)
c_end=document.cookie.length;var cvalue=myunescape(document.cookie.substring(c_start,c_end));return unescape(cvalue);}}
return"";}
function myunescape(str)
{str=""+str;while(true)
{var i=str.indexOf('+');if(i<0)
break;str=str.substring(0,i)+'%20'+
str.substring(i+1,str.length);}
return unescape(str);}
function ed(id){return document.getElementById(id);}
