textVal--;$("#"+Id).val(textVal);$("#ageOfChildText"+lastChar).html("Age"+(textVal>1?"s":"")+" of Children");if(textVal==0){$("#ageWrapper"+lastChar).hide();}}
$("#guestRoomDrpdwn").html(calculateTotalGuest()+' guest'+(calculateTotalGuest()>1?'s':'')+' - '+totalRoomCount+' room'+(totalRoomCount>1?'s':''));}
