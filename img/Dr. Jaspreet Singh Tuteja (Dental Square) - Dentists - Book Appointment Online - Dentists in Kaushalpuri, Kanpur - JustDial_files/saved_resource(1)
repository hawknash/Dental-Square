function validatenew(page)
{if(page=="search")
{var returnFlag=true;if($("#srcId").val()=="-1")
{var cityName=$('#srcName').val().toLowerCase();$.each(cityListJSON,function(index,value){if(cityName==value.value.toLowerCase())
{$("#srcId").val(value.id);return;}});if($("#srcId").val()=="-1")
{$("#srcErr").removeClass('dn');returnFlag=false;}}else
{$("#srcErr").addClass('dn');}
if($("#destId").val()=="-1")
{var cityName=$('#destName').val().toLowerCase();$.each(cityListJSON,function(index,value){if(cityName==value.value.toLowerCase())
{$("#destId").val(value.id);return;}});if($("#destId").val()=="-1")
{$("#destErr").removeClass('dn');returnFlag=false;}}else
{$("#destErr").addClass('dn');}
if($("#doj").val()=="Select"||$("#doj").val()==null)
{$("#dojErr").removeClass('dn');$("#dojErr").html('Select date of journey.');returnFlag=false;}else
{$("#dojErr").html('');$("#dojErr").addClass('dn');}
return returnFlag;}}
function getSourcesNew()
{var returnVal=false;$.ajax({url:DOMAIN+"functions/ajxRedbus.php?action=sources",type:"GET",dataType:"json",async:false,success:function(result){if(result.error.code==400)
{}else
{cityListJSON=result.results;returnVal=true;}
$('#srcName').autocomplete({appendTo:"#trfrm",source:result.results,autoFocus:true,minLength:2,select:function(event,ui){if(ui.item!=null)
{$("#srcId").val(ui.item.id);}else
{$("#srcId").val("-1");}
$("#destName").focus();}});$('#destName').autocomplete({appendTo:"#trto",source:result.results,autoFocus:true,minLength:2,select:function(event,ui){if(ui.item!=null)
{$("#destId").val(ui.item.id);}else
{$("#destId").val("-1");}
$("#doj").focus();}});$.ui.autocomplete.prototype._renderItem=function(ul,item){var term=this.term.split(' ').join('|');var re=new RegExp('('+term+')','gi');var t=item.label.replace(re,'<span class="hgl">$1</span>');return $('<li></li>').data('item.autocomplete',item).append('<a>'+t+'</a>').appendTo(ul);};$.ui.autocomplete.filter=function(array,term){var matcher=new RegExp("^"+$.ui.autocomplete.escapeRegex($.trim(term)),"i");return $.grep(array,function(value){return matcher.test(value.label||value.value||value);});};}});return returnVal;}
