function GetMovieResult(almo){var SelectCity=(getCookie('scity'))?getCookie('scity'):'Mumbai';$.ajax({url:WEBROOT+"webmain/ajxmain.php?vid=97&lvl=1&city="+SelectCity+"&json=1&new_logic=6&records=3&cases=GetLatestMovieName&recodeData="+$("a[id$='Movie']").length,async:true,success:function(result){SetMovieHotkey(result,almo);}});}
function SetMovieHotkey(Data,almo){if((cMovieHkey==0&&almo=='onload')||(almo=='start'&&cMovieHkey==1)||(almo=='shloc'&&cMovieHkey==1)){var SelectCity=(getCookie('scity'))?getCookie('scity'):'Mumbai';var d=eval('('+Data+')');if(d['0']!=2){$("a[id$='Movie']").each(function(key,value){if($("#ht_lk_sub_"+key+"_Movie").length){$("#ht_lk_sub_"+key+"_Movie").html(d[key]['name']).attr('href',d[key]['Url']).attr('title',d[key]['title']+' in '+SelectCity);}
$('#ht_lk_sub_mr_Movie').removeClass('dn');});}else{$("a[id$='Movie']").each(function(key,value){if($("#ht_lk_sub_"+key+"_Movie").length){if(key==1){$("#ht_lk_sub_"+key+"_Movie").html('Cinema Theatre').attr('href',DOMAIN+SelectCity+"/cinema-theatre");}else{$("#ht_lk_sub_"+key+"_Movie").html('');}}
$("#ht_lk_sub_mr_Movie").addClass('dn');});}
cMovieHkey=1;}}
