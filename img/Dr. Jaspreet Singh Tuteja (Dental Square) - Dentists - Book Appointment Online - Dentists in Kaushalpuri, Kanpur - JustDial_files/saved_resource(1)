{var c=$("#what").val().toLowerCase();var classstar=(c.length<70)?'cstars ':'stars';var html='';var ratetxt='';var urlpart='';urlpart=(!d.totRatings||d.totRatings.stars==0)?'?tab=writereview':'#rvw';if(window.location.href.indexOf("#rvw")>-1){urlpart='';}
