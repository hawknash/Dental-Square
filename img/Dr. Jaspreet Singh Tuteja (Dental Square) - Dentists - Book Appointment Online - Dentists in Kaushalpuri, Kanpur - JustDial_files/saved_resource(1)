function sendverificationcode(username,mobile,vcode,rsend,flg,rdflg){$.ajax({url:DOMAIN+"functions/whatsappverification.php",data:{'mob':mobile,'vcode':vcode,'rsend':rsend,'name':username},type:'POST',success:function(data){var html="";var res_arr=JSON.parse(data);$('.otp-number').html('+91 '+mobile);res_arr.msg=res_arr.msg.replace(/\_/g,' ');if(res_arr.code==0)
{if(flg==1||flg==2)
{$('.otp-msg').removeClass('dn');html+="<span class='otp-tickmark'></span>";html+="<span class='otp-msg-text'>Verification message sent to +91 "+mobile+"</span>";$('.otp-msg').html(html);return false;}}
else if(res_arr.code==3)
{$('.otp-msg').removeClass('dn');html+="<span class='otp-tickmark'></span>";html+="<span class='otp-msg-text'>successfully Verified</span>";$('.otp-msg').html(html);window.location.reload();}
else if(res_arr.code==1)
{$('.tLOtp_error').show();$('.tLOtp_error').html(res_arr.msg);return false;}else if(res_arr.code==4)
{$('.tLOtp_error').show();$('.tLOtp_error').html(res_arr.msg);}}});}
