{for(j=0;j<n.length;j++)
{if(typeof(d[0][n[j]])!='undefined'&&d[0][n[j]]['jd_movieid']==mid)
{var temp=n[j];var idxtemp=j;}}
if(mid!=''&&typeof(temp)!='undefined')
{n.splice(idxtemp,1);n.splice(0,0,temp);}
