{if(mobile_req.readyState==4)
{if(mobile_req.responseText=='SUCCESS')
{errmsg='Verification code sent successfully';}
else if(mobile_req.responseText=='FAILED')
{errmsg='Code sent failed! please try again';}
else if(mobile_req.responseText=='LIMIT_REACHED')
{errmsg='Daily sms limit reached';}
if(modName=='enqsmseml'){$('#rtpvc_errors').show();$('#rtpvc_errors').html('Verification code sent successfully!!');}else{if($('#resend_msg_bat'))
{$('#resend_msg_bat').show();$('#resend_msg_bat').html(errmsg);$('#btvcodeErr').hide();$('#btvcodeErr').html('');}
else
{$('#resend_msg').show();$('#resend_msg').html(errmsg);}}
return false;}
return false;};mobile_req.open("GET",DOMAIN+"functions/resend_mobile_verification_code.php?mobile="+mobile_no+"&module="+modName);mobile_req.setRequestHeader("Content-Type","application/x-www-form-urlencoded;charset=iso-8859-1");mobile_req.send(null);}
