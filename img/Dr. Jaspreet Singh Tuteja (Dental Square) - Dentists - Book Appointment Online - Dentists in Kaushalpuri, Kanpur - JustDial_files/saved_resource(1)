{html+="<div class='holdiqwrp'><div class='holdiq'><a  href='"+d.hiq_url+"' target='_blank' rel='nofollow' ><span class='lng_hq_rat'>More Reviews from HolidayIQ.com</span> >></a></div>";}
if(lastpage>1&&BBLKS==0)
{var c;html+="<span class='jpag' id='"+idn+"'>";prev=page-1;if(page>1)
{html+='<a href="#rvw" onclick="'+fn+'('+prev+','+totrev+',\'\','+d.paid+',\''+d.jsreviewdocids+'\',\''+d.company_name+'\');"><span>&lsaquo;&lsaquo;</span> Prev</a>';}else
{html+='<span class="dis"><span>&lsaquo;&lsaquo;</span> <span class="lng_hq_rat">Prev</span></span>';}
if(lastpage<7+(d.adjacents*2))
{for(c=1;c<=lastpage;c++)
{if(c==page)
{html+='<span class="act">'+c+'</span>';}else
{html+='<a href="#rvw" onclick="'+fn+'('+c+','+totrev+',\'\','+d.paid+',\''+d.jsreviewdocids+'\',\''+d.company_name+'\');">'+c+'</a>';}}}else if(lastpage>5+(d.adjacents*2))
{if(page<1+(d.adjacents*2))
{for(c=1;c<4+(d.adjacents*2);c++)
{if(c==page)
{html+='<span class="act">'+c+'</span>';}else
{html+='<a href="#rvw" onclick="'+fn+'('+c+','+totrev+',\'\','+d.paid+',\''+d.jsreviewdocids+'\',\''+d.company_name+'\');">'+c+'</a>';}}
