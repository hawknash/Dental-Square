{show_rate_this=1;if(d.totRatings&&d.totRatings.total>=0&&d.showstars==1)
{if(d.totRatings.total==0||d.totRatings.total==null)
{if(d.totRatings.paid>0)
{$('.count').html(d.totRatings.modtotal);$('.rtext').html('Ratings');}else
{$('.count').html();$('.rtext').html('');}}else if(d.totRatings.total==1)
{$('.count').html(d.totRatings.modtotal);if(d.totRatings.total==1&&d.totRatings.paid<1)
