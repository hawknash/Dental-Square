function getRelatedKeywords()
{var catid=$("#catid").val();var type=$("#type").val();var tmp_iwhat=encodeURIComponent(iwhat);var tmp_related_keyword_key=encodeURIComponent(related_keyword_key);var citi=encodeURIComponent(getCookie('main_city'));$.ajax({url:WEBROOT+"functions/getRelatedKeywords.php?catid="+catid+"&type="+type+"&text="+tmp_iwhat+"&area="+res_area+"&key="+tmp_related_keyword_key+"&city="+citi+"&version="+VERSION,async:true,success:function(result)
