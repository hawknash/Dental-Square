function guestIncrement(Id){var textVal=$("#"+Id).val();var len=Id.length;var lastChar=Id.charAt(len-1);var rest=Id.slice(0,len-1);var persons=6;if(rest=="dChildrenCountSelector_Htls"){var persons=3;}
