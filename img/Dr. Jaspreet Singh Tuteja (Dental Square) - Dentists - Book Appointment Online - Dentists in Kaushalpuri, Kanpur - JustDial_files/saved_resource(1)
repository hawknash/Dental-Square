function rev_redirect_detail(t,contesturl)
{var cflag=0;var url=location.href;if(contesturl!=''&&contesturl!=undefined){window.open(contesturl,'_blank');}
if(writeReviewMovie==1){url=url.replace(/\/rev55|\/rev66|\/rev77|\/rev88|\/rev99|\/4jf$/,'');}else if(writeReviewProd==1){url=url.replace(/\/rev55|\/rev66|\/rev77|\/rev88|\/rev99|\/4jf$/,'');if(t=='')
{str=$('#psearch').val();str=str.replace(/ /g,"-");str=str.replace(/--/g,"-");url=DOMAIN+str+'/pid-'+$('#prid').val();}}else{url=url.replace(/\\?tab=rev5|\\?tab=rev6|\\?tab=1|\\?tab=2|\\?tab=3|\\?tab=4|\\?tab=moreinfo|\\?tab=map|\\?tab=rev7|\\?tab=rev8|\\?tab=rev9|\\?tab=rev10|\/4jf$/,'');if(url.indexOf("?xid=")){}else{url=url.replace(/[?]/g,'');}
url+=(t!='')?'?tab='+t:'';}
var urlsplit=location.href.split('/');if(url.indexOf('_review')!=-1)
{var changeurl=url.replace(DOMAIN,'');changeurl=changeurl.replace('_reviews','');var changeurlarr=changeurl.split('/');changeurlarr[0]='Movies';if(changeurlarr[3])
url=DOMAIN+changeurlarr.join('/');else
{var endid=changeurlarr[2].split('_');changeurlarr[2]=endid[0];if(endid[1])
url=DOMAIN+changeurlarr.join('/')+'?xid='+endid[1];else
url=DOMAIN+changeurlarr.join('/');}}
if(window.event){window.event.returnValue=false;window.location=url;return false;}else{window.location=url;}}
