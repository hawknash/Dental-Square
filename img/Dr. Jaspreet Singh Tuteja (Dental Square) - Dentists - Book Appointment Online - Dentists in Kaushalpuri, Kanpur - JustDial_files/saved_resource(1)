function showRoomdata(roomCode){if($("#chkArrow"+roomCode).hasClass('accord-drpDown')){$("#chkArrow"+roomCode).removeClass('accord-drpDown');$("#chkArrow"+roomCode).addClass('accord-drpUp');$("#RoomData"+roomCode).hide();}else{$("#chkArrow"+roomCode).removeClass('accord-drpUp');$("#chkArrow"+roomCode).addClass('accord-drpDown');$("#RoomData"+roomCode).show();}}
function fetchRoomData(data,code,name,type,planCode){var rRate="";var rMedia=JSON.parse(data);var rMedia1="";var roomDtlArr=[];switch(type){case"RI":rRate=rMedia.reSearch.results[0].room_stays[0].RatePlans;if(rRate.hasOwnProperty("Inclusions")){rRate[0]=rRate;}
for(var i=0;i<rRate.length;i++){if(planCode==rRate[i].code){roomDtlArr.push(rRate[i]);}}
break;case"RM":if(rMedia.reSearch.results[0].room_media!=""&&typeof rMedia.reSearch.results[0].room_media!="undefined"){rMedia1=JSON.parse(rMedia.reSearch.results[0].room_media);for(var key in rMedia1){if(code===rMedia1[key].RoomCode){roomDtlArr.push(rMedia1[key].Src);}}}else{roomDtlArr.push([]);}
break;case"RA":if((rMedia.reSearch.results[0]).hasOwnProperty("roominfo")&&rMedia.reSearch.results[0].roominfo!=""){var rMinfo=JSON.parse(rMedia.reSearch.results[0].roominfo);for(var key in rMinfo){if(name===rMinfo[key].RoomDescription&&typeof rMinfo[key].Amenities!="undefined"){roomDtlArr.push(rMinfo[key].Amenities);}}}
break;case"RT":var rRate=rMedia.reSearch.results[0].room_stays[0].RoomTypes[0];for(var key in rRate){if(code==rRate.roomTypeCode&&planCode==rRate.ratePlanCode){roomDtlArr.push(rRate.Rate.CancelPenalties[0].PenaltyDescription.value);}}
break;case"PD":var rRate=rMedia.reSearch.results[0].room_stays[0].RoomRates;roomDtlArr=[];for(var i=0;i<rRate.length;i++){if(code==rRate[i].roomTypeCode){roomDtlArr.push(rRate[i]);}}
break;case"GDS":var rRate=rMedia.reSearch.results[0];roomDtlArr=[];for(var i=0;i<rRate.length;i++){if(code==rRate[i].roomTypeCode){roomDtlArr.push(rRate[i]);}}
break;}
return roomDtlArr;}
